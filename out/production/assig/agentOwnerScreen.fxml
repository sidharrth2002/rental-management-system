<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="AgentOwnerScreenController"
      prefHeight="400.0" prefWidth="600.0" fx:id="root"
     style="-fx-background-color: white">
    <MenuBar>
        <menus>
            <Menu text="Options">
                <items>
                    <MenuItem fx:id="roleChooser" text="Role Chooser" onAction="#changeScene" />
                    <MenuItem fx:id="registerPage" text="Register" onAction="#changeScene" />
                    <MenuItem fx:id="saveAndExit" text="Save And Exit" onAction="#changeScene" />
                </items>
            </Menu>
        </menus>
    </MenuBar>

    <VBox alignment="center" style="-fx-padding: 10 10 10 10;" spacing="20">
        <Text fx:id="welcomeMessage" style="-fx-font-size: 22"/>
        <Text fx:id="dateToday" style="-fx-font-size: 16; -fx-fill: red" />
        <Text text="Below, the property you manage is listed: " />

        <VBox fx:id="propertyList">

        </VBox>
    </VBox>
</VBox>